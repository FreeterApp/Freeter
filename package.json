{
  "name": "freeter",
  "version": "2.0.0-alpha",
  "main": "main.js",
  "homepage": "https://freeter.io/",
  "repository": "https://github.com/FreeterApp/Freeter.git",
  "author": "Alex Kaul",
  "license": "GPL-3.0-or-later",
  "description": "Organizer For Those Who Do",
  "scripts": {
    "dev": "rimraf build && yarn run dev:assets && yarn run dev:preload && concurrently --names \"RENDERER,MAIN,RUN\" -c \"blue.bold,magenta.bold,green.bold\" \"yarn run dev:renderer\" \"yarn run dev:main\" \"yarn run dev:run\"",
    "dev:assets": "copyfiles --up 1 src/assets/**/* build/",
    "dev:preload": "cross-env NODE_ENV=development webpack --config webpack.preload.config.js",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack.renderer.config.js",
    "dev:main": "cross-env NODE_ENV=development webpack --watch --config webpack.main.config.js",
    "dev:run": "cross-env NODE_ENV=development nodemon --on-change-only --watch build/main.js --exec \"sleep 3 && electron ./build/main.js\"",
    "prod": "rimraf build && yarn run prod:assets && concurrently --names \"RENDERER,MAIN,PRELOAD\" -c \"blue.bold,magenta.bold,green.bold\" \"yarn run prod:renderer\" \"yarn run prod:main\" \"yarn run prod:preload\"",
    "prod:assets": "copyfiles --up 1 src/assets/**/* build/",
    "prod:preload": "cross-env NODE_ENV=production webpack --config webpack.preload.config.js",
    "prod:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "prod:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "prod:run": "electron ./build/main.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --clearCache && cross-env JEST_HIDE_USAGE=1 jest --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "package:pack": "electron-builder --config electron-builder.config.js --dir",
    "package:dist": "electron-builder --config electron-builder.config.js",
    "release": "electron-builder --config electron-builder.config.js --publish onTagOrDraft"
  },
  "devDependencies": {
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^27.0.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.37.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "swc-loader": "^0.2.3",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "type-fest": "^4.4.0",
    "typescript": "^5.0.3",
    "typescript-plugin-css-modules": "^5.0.0",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "uuid": "^9.0.0",
    "zustand": "^4.3.7"
  }
}
