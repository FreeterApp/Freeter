{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ES6",
    "target": "ES2021",
    "jsx": "react-jsx",
    "baseUrl": "../",
    "paths": {
      "@/*": ["renderer/*"],
      "@common/*": ["common/*"]
    },
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }]
  }
}
